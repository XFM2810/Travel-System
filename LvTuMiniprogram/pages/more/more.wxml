
<view class="container more">
  <view class="user flex-wrp" bindtap="bindUserInfo">
    <view class="avatar flex-item">
      <image class="userinfo-avatar" src="{{userInfo.icon}}" background-size="cover"></image>
    </view>
    <view class="user-info flex-item">
      <text class="userinfo-nickname">{{userInfo.user_name}}</text>
      <text class="edit">查看或编辑个人主页</text>
    </view>
  </view>

  <view class="my">
    <view class="my-item flex-wrp" bindtap="bindMyTip1">
      <view class="myitem-icon flex-item" >
        <image src="../../images/eye.png"></image>
      </view>
      <view class="myitem-name flex-item">
        <text>我的攻略</text>
      </view>
    </view>
    <view class="my-item flex-wrp" bindtap="bindMyTip2" >
      <view class="myitem-icon flex-item" >
        <image src="../../images/star.png"></image>
      </view>
      <view class="myitem-name flex-item">
        <text>我的收藏</text>
      </view>
    </view>
    <view class="my-item flex-wrp" bindtap="bindWriteTip">
      <view class="myitem-icon flex-item" >
        <image src="../../images/draft.png"></image>
      </view>
      <view class="myitem-name flex-item">
        <text>发布攻略</text>
      </view>
    </view>
  </view>
</view>
